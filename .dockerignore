# Ignore local dependencies
node_modules

# Ignore build output, it will be generated inside the container
dist

# Ignore log files
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Ignore environment files - these should be injected at runtime, not built into the image
.env
.env.*

# Ignore Docker files themselves
Dockerfile
.dockerignore

# Ignore git files
.git
.gitignore
